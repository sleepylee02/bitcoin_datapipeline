# SBE Ingestor Service - Docker Environment
# Production environment variables for SBE Ingestor service

# ===== BINANCE API =====
BINANCE_API_KEY=${BINANCE_API_KEY}
BINANCE_API_SECRET=${BINANCE_API_SECRET}
BINANCE_SBE_WS_URL=wss://stream-sbe.binance.com/stream

# ===== AWS CONFIGURATION =====
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}
AWS_DEFAULT_REGION=${AWS_DEFAULT_REGION}
AWS_KINESIS_STREAM_TRADES=${AWS_KINESIS_STREAM_TRADES}
AWS_KINESIS_STREAM_DEPTH=${AWS_KINESIS_STREAM_DEPTH}
AWS_KINESIS_STREAM_TICKER=${AWS_KINESIS_STREAM_TICKER}

# ===== SERVICE CONFIGURATION =====
ENVIRONMENT=production
LOG_LEVEL=INFO
DEBUG=false

# ===== SBE INGESTOR SPECIFIC =====
SBE_INGESTOR_LOG_LEVEL=INFO
SBE_INGESTOR_RECONNECT_DELAY=5
SBE_INGESTOR_MAX_RECONNECTS=10
SBE_INGESTOR_BUFFER_SIZE=10000

# ===== HEALTH CHECK =====
HEALTH_CHECK_PORT=8080
METRICS_PORT=8081