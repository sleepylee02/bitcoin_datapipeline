# Environment Variables Example
# Copy this file to .env and update with your actual values
# NEVER commit .env files to version control

# Configuration Selection (optional - defaults provided)
CONFIG_FILE=config/local.yaml
ENVIRONMENT=local

# Binance API Credentials (required for SBE mode)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_API_SECRET=your_binance_api_secret_here

# AWS Credentials (for local development only - use IAM roles in production)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_SESSION_TOKEN=your_aws_session_token_here_if_using_temp_creds

# Database Credentials (if applicable)
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password
DB_HOST=your_db_host
DB_PORT=5432

# Redis Credentials (if applicable)
REDIS_PASSWORD=your_redis_password
REDIS_HOST=your_redis_host
REDIS_PORT=6379

# Other service API keys
MONITORING_API_KEY=your_monitoring_service_api_key
ALERT_WEBHOOK_URL=your_slack_or_teams_webhook_url